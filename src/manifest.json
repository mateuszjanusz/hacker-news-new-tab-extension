{
	"manifest_version": 3,
	"name": "Hacker News in New Tab",
	"version": "1.0.2",
	"description": "Hacker News in New Tab for quick access to the latest stories.",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"permissions": ["storage"],
	"host_permissions": [
		"https://hacker-news.firebaseio.com/*"
	],
	"icons": {
		"16": "img/hn16.png",
		"48": "img/hn48.png",
		"128": "img/hn128.png"
	}
}
